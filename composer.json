{
    "name": "lavamake/lavamake",
    "description": "lavamake cms core",
    "keywords": [
        "laravel",
        "lavamake",
        "cms"
    ],
    "authors": [
        {
            "name": "lavamake",
            "email": "maker68@163.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "laravel/framework": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Lavamake\\Lavamake\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lavamake\\Lavamake\\Providers\\LavamakeServiceProvider"
            ],
            "aliases": {
                "ArticleManage": "Lavamake\\Lavamake\\Facades\\ArticleManage",
                "LavaArticle": "Lavamake\\Lavamake\\Facades\\LavaArticleWeb",
                "LavaNav": "Lavamake\\Lavamake\\Facades\\LavaNavWeb"
            }
        }
    }
}
